<% if @sub_user.errors.present? %>
swal({
    icon: "error",
    title: "oops..!",
    buttons: false,
    timer: 3000,
    text: "<%= @sub_user.errors.full_messages.join('.') %>",
    html: true
});
<% else %>
swal({
    icon: "success",
    title: "Success",
    buttons: false,
    timer: 3000,
    text: "User created successfully"}).then(function(){
//    window.location = '<%#= settings_path %>'

});
$('#users_listing').html('<%= escape_javascript render(template: 'sub_users/settings_listing',
   layout: false, format: :html) %>');
initUserListingEvents();
showSuccessMsg('<%= t('views.users.saved_msg') %>')
<% end %>