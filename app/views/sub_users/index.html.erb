<%= render 'user_activity'%>
<div id="main-content" class='<%= "#{ 'side-show' if side_nav_opened?}"%>'>
  <!--- main content --->
  <div class="flex-wrapper">
    <!--- breadcrum Bar --->
    <div class="flex-item breadcrum-bar">
      <div class="card-white-panel ">
        <h3 class="with-grid-icons">
          <span><%= link_to t('views.users.users'), 'javascript:;', class: 'active' %></span>
          <%= link_to raw('<i class="material-icons">add</i>'), new_sub_user_path, remote: true,
                      class: 'plus-add-icon', title: t('helpers.links.add') %>
          <%= index_layout_toggle_icons(sub_users_path(layout_toggle_params(view: 'card')),
                                        sub_users_path(layout_toggle_params(view: 'table'))) %>
        </h3>
      </div>
    </div>
  </div>

  <div class="invoice-box">
    <div class="search_result_container">
      <%= render 'sub_users_container'%>
    </div>
  </div>

</div>