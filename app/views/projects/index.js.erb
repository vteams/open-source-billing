$('thead#iproject_head').html('<%= escape_javascript render("project_header") %>')
$('tbody#project_body').html('<%= escape_javascript render("project") %>');
//$(".sent, .partial,.draft, .draft-partial, .paid, .disputed, .viewed").qtip();
$('tfoot#paging_links').html('<%= escape_javascript render("application_partials/paging") %>');
$('#bulk_actions').html('<%= escape_javascript render("bulk_actions") %>');
$('#active_links').html('<%= escape_javascript render("filter_links") %>');
//$("table.table_listing").trigger('update');
$('#sort_direction').html('<%= params[:direction]%>');
// Add page params in table listing headers
<% status = params[:status]%>
$('.top_links').hide();
$('#active_links a').removeClass('active');
$('.primary_heading h1 span').html("&nbsp;/&nbsp;<%= status.capitalize%>");
<%if status == "active" %>
$('.archive,.destroy').show();
$('#active_links a.get_actives').addClass('active');
<% elsif status == "archived"%>
$('.recover_archived,.destroy_archived').show();
$('#active_links a.get_archived').addClass('active');
<% elsif status == "deleted"%>
$('.recover_deleted').show().parent('li').removeAttr('class');
$('#active_links a.get_deleted').addClass('active');
<%end%>
window.preventDeletedNavigation()
window.history.pushState({html: $("body").html()},"","<%= request.original_url %>")
bind_deleted_entry();